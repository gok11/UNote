            <ui:VisualElement name="NoteTag" style="flex-grow: 1; flex-direction: row; height: auto; margin-top: 2px; margin-right: 0; margin-bottom: 0; margin-left: 0;">
                <ui:VisualElement name="Tags" style="flex-grow: 1; flex-direction: row-reverse;" />
            </ui:VisualElement>
            <ui:Button text="â€¦" parse-escape-sequences="true" display-tooltip-when-elided="true" name="ContextButton" style="width: 20px; height: 18px; visibility: hidden; margin-top: 3px; margin-left: 2px;" />
